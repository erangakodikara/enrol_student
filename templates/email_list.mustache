{{!
    @template block_enrol_student/email_list

    This template renders the email list.

    Example context (json):
    {
       "users" [
                  [
                  "email" : s1@example.com
                   "firstname" " student_1
                    "lastname" : student_11
                  ],
                  [
                  "email" : s2example.com
                  "firstname" " student_2
                  "lastname" : student_22
                  ],
               ]
    }

  }}

<div class="container justify-content-center mb-3" width="0%">

    <h6>Student List</h6>
    <div class="row justify-content-start">

        <table class="enrol-student-container table table-bordered loading">
            <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Email</th>
            </tr>
            </thead>
            <tbody>
            </tbody>

        </table>
        <div class="student-block-pagination justify-content-center mb-3"></div>
    </div>

</div>

{{#js}}
    require(['block_enrol_student/main'], function(main) {
    main.init({{courseid}} ,{{perpage}});
    });
{{/js}}

