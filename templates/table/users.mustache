{{!
    @template block_enrol_student/table/users

    This template renders the users list.

    Example context (json):
    {
       "users" [
                  [
                  "email" : s1@example.com
                   "firstname" " student_1
                    "lastname" : student_11
                  ],
                  [
                  "email" : s2example.com
                  "firstname" " student_2
                  "lastname" : student_22
                  ],
               ]
    }

  }}

{{#users}}
    <tr class="studentlist">
        <td scope="col">{{ userid }}</td>
        <td scope="col">
            <a href="" id="user_id" value="{{userid}}" data-id="{{userid}}">{{ email }}</a>
        </td>
    </tr>
{{/users}}
